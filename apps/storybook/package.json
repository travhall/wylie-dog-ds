{
  "name": "storybook",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm build:css && storybook build --docs",
    "build:analyze": "pnpm build:css && ANALYZE=true storybook build --docs",
    "build:css": "tailwindcss -i ./src/styles/index.css -o ./dist/storybook.css --minify",
    "clean": "rm -rf .turbo node_modules dist",
    "dev": "pnpm build:css && storybook dev -p 6006",
    "dev:css:watch": "tailwindcss -i ./src/styles/index.css -o ./dist/storybook.css --watch",
    "lint": "eslint \"./stories/**/*.stories.tsx\" --max-warnings 0",
    "preview-storybook": "serve storybook-static"
  },
  "dependencies": {
    "@wyliedog/tokens": "workspace:*",
    "@wyliedog/ui": "workspace:*",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@repo/eslint-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-links": "^10.1.11",
    "@storybook/addon-vitest": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@storybook/test": "^8.6.15",
    "@tailwindcss/cli": "^4.1.18",
    "@tailwindcss/postcss": "^4.1.18",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-plugin-storybook": "10.1.11",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prettier-plugin-packagejson": "^2.5.20",
    "rollup-plugin-visualizer": "^6.0.5",
    "serve": "^14.2.5",
    "storybook": "^10.1.11",
    "storybook-design-token": "^5.0.0",
    "tailwindcss": "^4.1.18",
    "typescript": "5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
