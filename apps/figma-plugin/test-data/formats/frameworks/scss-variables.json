{
  "$primary-color": "#3b82f6",
  "$primary-50": "#eff6ff",
  "$primary-100": "#dbeafe",
  "$primary-500": "#3b82f6",
  "$primary-600": "#2563eb", 
  "$primary-900": "#1e3a8a",
  "$secondary-color": "#6366f1",
  "$neutral-50": "#f8fafc",
  "$neutral-100": "#f1f5f9",
  "$neutral-500": "#64748b",
  "$neutral-900": "#0f172a",
  "$success-color": "#10b981",
  "$warning-color": "#f59e0b",
  "$error-color": "#ef4444",
  "$base-font-size": "16px",
  "$small-font-size": "14px",
  "$large-font-size": "18px",
  "$heading-font-size": "24px",
  "$font-family-sans": "Inter, system-ui, sans-serif",
  "$font-family-mono": "JetBrains Mono, monospace",
  "$line-height-base": "1.5",
  "$line-height-tight": "1.25",
  "$line-height-relaxed": "1.75",
  "$font-weight-normal": "400",
  "$font-weight-medium": "500",
  "$font-weight-bold": "700",
  "$border-radius-sm": "0.125rem",
  "$border-radius-md": "0.375rem",
  "$border-radius-lg": "0.5rem",
  "$spacing-unit": "0.25rem",
  "$spacing-xs": "calc($spacing-unit * 1)",
  "$spacing-sm": "calc($spacing-unit * 2)", 
  "$spacing-md": "calc($spacing-unit * 4)",
  "$spacing-lg": "calc($spacing-unit * 6)",
  "$spacing-xl": "calc($spacing-unit * 8)",
  "$box-shadow-sm": "0 1px 2px rgba(0, 0, 0, 0.05)",
  "$box-shadow-md": "0 4px 6px -1px rgba(0, 0, 0, 0.1)",
  "$box-shadow-lg": "0 10px 15px -3px rgba(0, 0, 0, 0.1)",
  "$transition-duration": "0.15s",
  "$transition-timing": "cubic-bezier(0.4, 0, 0.2, 1)",
  "$z-index-dropdown": "1000",
  "$z-index-modal": "1050",
  "$z-index-tooltip": "1070",
  "$background-color": "$neutral-50",
  "$text-color": "$neutral-900",
  "$text-muted": "$neutral-500",
  "$border-color": "$neutral-200",
  "$link-color": "$primary-color",
  "$link-hover-color": "darken($primary-color, 10%)",
  "$button-padding-x": "$spacing-md",
  "$button-padding-y": "$spacing-sm",
  "$button-border-radius": "$border-radius-md",
  "$card-padding": "$spacing-lg",
  "$card-border-radius": "$border-radius-lg",
  "$card-background": "$background-color",
  "$card-shadow": "$box-shadow-md"
}