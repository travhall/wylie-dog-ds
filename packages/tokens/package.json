{
  "name": "@wyliedog/tokens",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./css": "./dist/tokens.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node style-dictionary.config.js && tsup",
    "dev": "node style-dictionary.config.js --watch & tsup --watch",
    "clean": "rm -rf dist",
    "sync:figma": "node scripts/figma-sync.js",
    "generate:types": "node scripts/generate-types.js",
    "test:tokens": "node scripts/validate-tokens.js",
    "migration:w3c": "node scripts/migrate-to-w3c.js",
    "validate:accessibility": "node scripts/validate-accessibility.js"
  },
  "dependencies": {
    "culori": "^4.0.2"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@tokens-studio/sd-transforms": "^2.0.1",
    "style-dictionary": "^5.0.1",
    "tsup": "^8.5.0"
  }
}
