{
  "name": "@wyliedog/tokens",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./tokens.css": "./dist/tokens.css",
    "./semantic-light.css": "./dist/semantic-light.css",
    "./semantic-dark.css": "./dist/semantic-dark.css",
    "./hierarchical": "./dist/hierarchical.js",
    "./manifest.json": "./dist/manifest.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "audit": "node scripts/audit-tokens.js",
    "audit:verbose": "node scripts/audit-tokens.js --verbose",
    "build": "pnpm process-io && node style-dictionary.config.js && node scripts/generate-font-loaders.js",
    "build:tokens": "pnpm build",
    "clean": "rm -rf dist io/processed",
    "dev": "node scripts/watch-tokens.js",
    "export-demo-tokens": "node scripts/export-demo-tokens.mjs",
    "generate:token-map": "node scripts/generate-token-map.js --markdown",
    "process-io": "node scripts/process-token-io.js",
    "report:tokens": "node scripts/report-token-usage.mjs",
    "report:usage": "node scripts/analyze-component-usage.js",
    "test:tokens": "node scripts/validate-tokens.js"
  },
  "dependencies": {
    "culori": "^4.0.2"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@tokens-studio/sd-transforms": "^2.0.1",
    "chokidar": "^5.0.0",
    "style-dictionary": "^5.0.1",
    "tsup": "^8.5.0"
  }
}
