{
  "name": "@wyliedog/tokens",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./css": "./dist/tokens.css",
    "./theme": "./dist/theme.css",
    "./tailwind": "./dist/tailwind.js"
  },
  "files": [
    "dist",
    "src/tokens.generated.ts"
  ],
  "scripts": {
    "build": "node style-dictionary.config.js && tsup",
    "dev": "node style-dictionary.config.js --watch & tsup --watch",
    "clean": "rm -rf dist",
    "sync:figma": "node scripts/figma-sync.js",
    "generate:types": "node scripts/generate-types.js",
    "test:tokens": "node scripts/validate-tokens.js"
  },
  "dependencies": {
    "culori": "^4.0.2"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "style-dictionary": "^4.0.1",
    "tsup": "^8.0.2"
  }
}