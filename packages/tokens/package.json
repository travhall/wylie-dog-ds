{
  "name": "@wyliedog/tokens",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./src/*": "./src/*.json",
    "./tokens.css": "./dist/tokens.css",
    "./primitives.css": "./dist/primitives.css",
    "./semantic-light.css": "./dist/semantic-light.css",
    "./semantic-dark.css": "./dist/semantic-dark.css",
    "./hierarchical": "./dist/hierarchical.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
