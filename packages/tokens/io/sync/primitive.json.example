{
  "_comment1": "This shows examples of font token configurations using @fontSource pattern",
  "_comment2": "The @fontSource(...) pattern in $description enables font loader generation",
  "_comment3": "Copy the pattern you need into your actual primitive.json",

  "examples": {
    "_google_fonts": "Google Fonts - most common case",
    "typography.font-family.sans": {
      "$type": "fontFamily",
      "$value": "Inter",
      "$description": "@fontSource(provider:google,weights:400-500-600-700-800,subsets:latin-latin-ext,display:swap)"
    },

    "_local_fonts": "Local font files - self-hosted",
    "typography.font-family.custom": {
      "$type": "fontFamily",
      "$value": "CustomFont",
      "$description": "@fontSource(provider:local,fonts:[{\"weight\":\"400\",\"style\":\"normal\",\"path\":\"/fonts/CustomFont-Regular.woff2\"},{\"weight\":\"700\",\"style\":\"normal\",\"path\":\"/fonts/CustomFont-Bold.woff2\"}],display:swap)"
    },

    "_adobe_fonts": "Adobe Fonts (Typekit)",
    "typography.font-family.adobe": {
      "$type": "fontFamily",
      "$value": "acumin-pro",
      "$description": "@fontSource(provider:adobe,kitId:abc1234,weights:400-600-700)"
    },

    "_custom_cdn": "Custom CDN or font hosting service",
    "typography.font-family.hosted": {
      "$type": "fontFamily",
      "$value": "HostedFont",
      "$description": "@fontSource(provider:url,stylesheet:https://fonts.mycdn.com/hosted-font.css)"
    },

    "_system_fonts": "System font stack - no loading needed",
    "typography.font-family.system": {
      "$type": "fontFamily",
      "$value": "-apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
      "$description": "@fontSource(provider:system)"
    },

    "_variable_fonts": "Variable font with custom axes",
    "typography.font-family.variable": {
      "$type": "fontFamily",
      "$value": "Inter Variable",
      "$description": "@fontSource(provider:google,isVariable:true,subsets:latin,display:swap)"
    },

    "_multilingual": "Subset-optimized for specific languages",
    "typography.font-family.multilingual": {
      "$type": "fontFamily",
      "$value": "Noto Sans",
      "$description": "@fontSource(provider:google,weights:400-700,subsets:latin-cyrillic-greek-vietnamese,display:swap,preload:latin)"
    }
  }
}
